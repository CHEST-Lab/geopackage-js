    "email": "daniel.barela@gmail.com"
  },
  "homepage": "https://www1.nga.mil",
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.2.0"
  },
  "dependencies": {
    "async": "^1.5.0",
    "bufferstream": "^0.6.2",
    "file-type": "^3.8.0",
    "java": "^0.6.0",
    "jquery": "^2.2.3",
    "leaflet": "^0.7.7",
    "lwip": "0.0.8",
    "node-java-maven": "0.0.11",
    "proj4": "^2.3.14",
    "proj4js-defs": "0.0.1",
    "q": "^1.4.1",
    "reproject": "^1.1.1",
    "simple-bufferstream": "^1.0.0",
    "sql.js": "^0.3.2",
    "sqlite3": "^3.1.2",
    "stream-to-array": "^2.1.0",
    "uglify-js": "^2.6.2",
    "wkx": "^0.2.0"
  },
  "java": {
    "dependencies": [
      {
        "groupId": "mil.nga.geopackage",
        "artifactId": "geopackage",
        "version": "1.1.3"
      }
    ]
  },
  "bundleDependencies": [],
  "private": true,
  "main": "geopackage.js",
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.1",
    "browserify-istanbul": "^2.0.0",
    "browserify-shim": "^3.8.12",
    "chai": "^3.5.0",
    "image-diff": "^1.5.1",
    "ink-docstrap": "^1.1.4",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.1",
    "karma-ng-html2js-preprocessor": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.23",
    "mocha": "^2.4.5",
    "mochawesome": "^1.3.2",
    "testling": "^1.7.1",
    "watchify": "^3.7.0"
  },
  "testling": {
    "harness": "mocha",
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "scripts": {
    "test": "istanbul cover  _mocha",
    "mocha": "mocha",
    "btest": "karma start",
    "predoc": "rm -rf docs/*",
    "doc": "jsdoc --verbose -c jsdoc.conf",
    "build": "browserify -t browserify-css index.js --standalone geopackage --exclude sqlite3 --exclude lwip -o static/geopackage.js",
    "postbuild": "uglifyjs --stats static/geopackage.js --output static/geopackage.min.js",
    "watch": "watchify -t browserify-css index.js -v --standalone geopackage --exclude sqlite3 --exclude lwip -o static/geopackage.js"
  }
}
