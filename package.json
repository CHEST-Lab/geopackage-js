{
  "name": "@ngageoint/geopackage",
  "version": "2.1.1",
  "description": "GeoPackage JavaScript Library",
  "keywords": [
    "NGA",
    "geopackage"
  ],
  "license": "MIT",
  "author": {
    "name": "NGA",
    "url": "https://www.nga.mil"
  },
  "contributors": [
    "Daniel Barela <daniel.barela@gmail.com>"
  ],
  "homepage": "http://ngageoint.github.io/geopackage-js/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngageoint/geopackage-js.git"
  },
  "bugs": {
    "url": "https://github.com/ngageoint/geopackage-js/issues"
  },
  "bin": "./cli",
  "dependencies": {
    "@mapbox/tilebelt": "^1.0.1",
    "@mapbox/vector-tile": "^1.3.1",
    "@turf/bbox": "^3.14.0",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/boolean-within": "^6.0.1",
    "@turf/circle": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/intersect": "^6.1.2",
    "@turf/line-intersect": "^6.0.2",
    "@turf/point-to-line-distance": "^6.0.0",
    "@turf/polygon-to-line": "^6.0.3",
    "better-sqlite3": "danielbarela/better-sqlite3#v5.4.4",
    "canvas": "2.6.0",
    "concat-stream": "^1.6.0",
    "d3-geo": "^1.10.0",
    "file-type": "8.1.0",
    "geojson-vt": "^3.2.0",
    "image-size": "^0.8.2",
    "pbf": "^3.1.0",
    "proj4": "2.6.0",
    "rbush": "2.0.2",
    "reproject": "1.2.2",
    "rtree-sql.js": "1.0.0",
    "vt-pbf": "^3.1.1",
    "webworkify": "^1.1.0",
    "wkx": "0.4.8"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/chai": "4.2.5",
    "@types/concat-stream": "^1.6.0",
    "@types/d3-geo": "^1.11.1",
    "@types/fs-extra": "^8.0.1",
    "@types/geojson": "^7946.0.7",
    "@types/mocha": "5.2.7",
    "@types/node": "^12.12.12",
    "@types/pbf": "^3.0.2",
    "@types/proj4": "^2.5.0",
    "@types/source-map-support": "^0.5.0",
    "babel-polyfill": "^6.23.0",
    "browserify": "^14.1.0",
    "browserify-css": "^0.10.0",
    "canvas-compare": "https://github.com/caldwellc/canvas-compare.git",
    "chai": "4.2.0",
    "coveralls": "^3.0.0",
    "fs-extra": "8.1.0",
    "json-loader": "^0.5.7",
    "lcov-badge": "^1.0.4",
    "mocha": "6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^10.0.2",
    "nyc": "^13.1.0",
    "source-map-support": "0.5.16",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "typedoc": "^0.15.4",
    "typescript": "^3.7.2",
    "uglify-es": "^3.0.27",
    "watchify": "^3.7.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "xhr-mock": "^2.4.1"
  },
  "optionalDependencies": {
    "chalk": "^1.1.3",
    "inquirer": "^1.0.2"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "static/geopackage.js",
    "cli",
    "lib",
    "converters",
    "dist"
  ],
  "browser": {
    "sqlite3": false
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "temp-dir": "./.test_run",
    "report-dir": "./docs/coverage",
    "reporter": [
      "lcov"
    ]
  },
  "scripts": {
    "test-mocha": "npm run clean && mocha",
    "gh-pages-build": "npm run create --prefix docs",
    "postgh-pages-build": "npm run doc --prefix jsdoc && rm -rf ./docs/node_modules",
    "clean": "rm -rf ./.test_run; rm -rf ./.nyc_output; rm -rf ./docs/coverage",
    "test-sqlnode": "nyc --no-clean mocha -r ts-node/register",
    "test-sqljs": "FORCE_SQLJS=true nyc --no-clean mocha -r ts-node/register",
    "build-browser-test": "browserify ./test/test.js --standalone tests --exclude better-sqlite3 --exclude inquirer  --exclude canvas -o bundle/test.bundle.js -d",
    "test-all": "npm run clean && npm run test-sqlnode && npm run test-sqljs && npm run report",
    "test": "npm run clean && npm run test-sqlnode && npm run report",
    "report": "nyc report",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc",
    "typedoc": "rm -rf ./docs-ts && typedoc",
    "ts-test": "mocha -r ts-node/register",
    "webpack": "node_modules/.bin/webpack --config webpack.config.js",
    "build-browserify": "browserify -t browserify-css ./built/index.js --standalone geopackage --exclude better-sqlite3 --exclude inquirer --exclude canvas -o static/geopackage.js",
    "watch": "watchify -t browserify-css index.js -v --standalone geopackage --exclude better-sqlite3 --exclude inquirer --exclude canvas -o static/geopackage.js",
    "no-prepublish": "mkdir dist; browserify -t browserify-css index.js --standalone geopackage --exclude better-sqlite3 --exclude inquirer --exclude canvas -o dist/geopackage.js && uglifyjs dist/geopackage.js > dist/geopackage.min.js",
    "prepare": "tsc"
  }
}
